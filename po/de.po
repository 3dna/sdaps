#, python-format
msgid ""
"Unknown file type (%s). questionnaire_odt should be application/vnd.oasis."
"opendocument.text"
msgstr ""
"Unbekanntes Dateiformat (%s). questionnaire_odt muss application/vnd.oasis."
"opendocument.text sein."

#: ../sdaps/setup/setup.py:47
#, python-format
msgid "Unknown file type (%s). questionnaire_pdf should be application/pdf"
msgstr ""
"Unbekanntes Dateiformat (%s). questionnaire_pdf muss application/pdf sein."

#: ../sdaps/setup/setup.py:54
#, python-format
msgid "Unknown file type (%s). additionalqobjects should be text/plain"
msgstr "Unbekanntes Dateiformat (%s). additionalqobjects muss text/plain sein."

#: ../sdaps/stamp/__init__.py:23
#, python-format
msgid ""
"stamp [count [used_ids]]\n"
"\t\n"
"\tStamp corner marks and questionnaire ids on the questionnaire.\n"
"\t\n"
"\tcount: number of unique questionnaire ids you want to create\n"
"\tused_ids: don't use any id named in this file (pattern = '%i\\n')\n"
"\t\n"
"\tcreates stamped_[index].pdf\n"
"\t"
msgstr ""
"stamp [count [used_ids]]\n"
"\t\n"
"\tStempelt Eckmarker und IDs auf den Fragebogen.\n"
"\t\n"
"\tcount: Die Anzahl an eindeutigen Fragebögen die erstellt werden soll\n"
"\tused_ids: Datei mit Fragebogen IDs die schon benutzt wurden (Muster = '%i"
"\\n')\n"
"\t\n"
"\tErstellt: stamped_[index].pdf\n"
"\t"

