#: ../sdaps/ids.py:22
#, python-format
msgid ""
"ids [pattern = '%i\\n']\n"
"\t\n"
"\tWrite a list containing all questionnaire ids.\n"
"\t\n"
"\tcreates ids_[index]\n"
"\t"
msgstr ""
"ids [pattern = '%i\\n']\n"
"\t\n"
"\tSchreibt eine Liste mit alle Fragebogen IDs.\n"
"\t\n"
"\tErstellt ids_[index]\n"
"\t"

#: ../sdaps/info.py:22
msgid ""
"info field [content]\n"
"\t\n"
"\tAlter metadata.\n"
"\t\n"
"\tfield: name of the field you whish to alter\n"
"\tcontent: the new content for that field\n"
"\t\t(empty if you want to delete the field)\n"
"\t"
msgstr ""
"info field [content]\n"
"\t\n"
"\tMetadata Editor.\n"
"\t\n"
"\tfield: Das Feld, dass verändert werden soll.\n"
"\tcontent: Der neue Inhalt des Feldes. Wenn dies übersprungen\n"
"\t\twird, dann fragt SDAPS nach.\n"
"\t"

