#: ../sdaps/csvdata/__init__.py:23
msgid ""
"csvdata export [filter...]\n"
"\t\n"
"\tExport to csv\n"
"\t\n"
"\tfilter: filter expression to select the sheets to export\n"
"\t\n"
"\tcreates data_[index].csv\n"
"\n"
"\n"
"csvdata import filename\n"
"\n"
"\tImport from csv\n"
"\n"
"\tfilename: file to read from\n"
"\t"
msgstr ""
"csvdata export [filter...]\n"
"\t\n"
"\tCSV Export\n"
"\t\n"
"\tfilter: Filter um nur einen Teil der Bögen zu exportieren\n"
"\t\n"
"\tErstellt data_[index].csv\n"
"\n"
"\n"
"csvdata import Dateiname\n"
"\n"
"\tImportiert Daten aus einer CSV Datei\n"
"\n"
"\tDateiname: file to read from\n"
"\t"

